{
  "name": "@lizzi/template",
  "version": "0.1.5",
  "description": "lizzi js template DOM engine",
  "author": "Stanislav Shishankin",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist/",
    "test": "jest",
    "build": "npm run clean && npm test && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src",
    "*.d.ts"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "source": "src/index.ts",
  "dependencies": {
    "@lizzi/core": "^0.1.4",
    "@lizzi/jsx-runtime": "^0.1.5",
    "@lizzi/node": "^0.1.4"
  },
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.5.0",
    "ts-jest": "^29.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lizzi-js/lizzi.git"
  },
  "keywords": [
    "lizzi",
    "snapshot"
  ],
  "bugs": {
    "url": "https://github.com/lizzi-js/lizzi/issues"
  },
  "homepage": "https://github.com/lizzi-js/lizzi#readme"
}
